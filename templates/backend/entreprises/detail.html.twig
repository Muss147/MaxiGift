<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyAGvHAv2FM7u0y-MEhQ456v8J5CwMQD_Ss"></script>
<script type="text/javascript">
const button = document.getElementById('kt_modal_delete_entreprise');
const formDelete = document.querySelector('#kt_delete_entreprise_form');

button.addEventListener('click', e =>{
    e.preventDefault();

    Swal.fire({
        html: "Êtes-vous sûr de vouloir supprimer <strong>{{entreprise.nom}}</strong>!",
        icon: "warning",
        buttonsStyling: false,
        showCancelButton: true,
        confirmButtonText: "Oui, supprimer!",
        cancelButtonText: 'Non, fermer',
        customClass: {
            confirmButton: "btn btn-danger",
            cancelButton: 'btn btn-light'
        }
    }).then(function (result) {
        if (result.value) {
            setTimeout(function () {
                Swal.fire({
                    html: "Entreprise supprimé!<br>Vous serez redirigé vers la liste des Entreprises.",
                    icon: "success",
                    buttonsStyling: false,
                    confirmButtonText: "Ok, compris!",
                    customClass: {
                        confirmButton: "btn fw-bold btn-primary",
                    }
                }).then(function () {
                    
                });
                formDelete.submit(); // Submit form
            }, 3000);
        } else if (result.dismiss === 'cancel') {
            Swal.fire({
                text: "L'Entreprise n'a pas été supprimée.",
                icon: "error",
                buttonsStyling: false,
                confirmButtonText: "Ok, compris!",
                customClass: {
                    confirmButton: "btn fw-bold btn-primary",
                }
            });
        }
    });
});

function checkEmail() {
    var email = $('#kt_modal_add_user_form input[name=email]').val();
    let url = "{{ path('admins.checkEmail') }}";
    $.ajax({
        url: url,
        type: "post",
        dataType: 'json',
        async: true,
        data: {
            'email': email
        }
    })
    .done(function(data){
        if (data) {
            $('.fv-plugins-message-container.text_msg').html('<div data-field="email" data-validator="emailAddress">Cet Email existe déjà, veuillez en choissir un autre.</div>');
            $('#kt_modal_add_user_form button[type=submit]').addClass('disabled');
        }
        else {
            $('.fv-plugins-message-container.text_msg').html('');
            $('#kt_modal_add_user_form button[type=submit]').removeClass('disabled');
        }
    });
}

function initializeAutocomplete(id) {
    var element = document.getElementById(id);
    if (element) {
        var autocomplete = new google.maps.places.Autocomplete(element, {types: ['geocode']});
        google.maps.event.addListener(autocomplete, 'place_changed', onPlaceChanged);
    }
}

function onPlaceChanged() {
    var place = this.getPlace();

    // console.log(place);  // Uncomment this line to view the full object returned by Google API.
    $('.gglocation').show();
    // $('input[name=lat]').val(place.geometry.location.lat());
    // $('input[name=long]').val(place.geometry.location.lng());

    for (var i in place.address_components) {
        var component = place.address_components[i];
        for (var j in component.types) { // Some types are ["country", "political"]
            var type_element = document.getElementById(component.types[j]);
            if (type_element) {
                type_element.value = component.long_name;
                // console.log('c', type_element)
            }
        }
    }
}

google.maps.event.addDomListener(window, 'load', function () {
    initializeAutocomplete('adresse_entreprise');
});
</script>
